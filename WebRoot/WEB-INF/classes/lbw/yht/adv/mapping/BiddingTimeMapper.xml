<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="lbw.yht.adv.dao.IBiddingTimeDao">

	<resultMap type="lbw.yht.adv.domain.BiddingTime" id="biddingTimeMap">
		<id column="bidding_time_id" property="biddingTimeId" />
		<result column="bidding_id" property="biddingId" />
		<result column="start_time" property="startTime" />
		<result column="end_time" property="endTime" />
		<result column="total_number" property="totalNumber" />
	</resultMap>

	<insert id="save" parameterType="lbw.yht.adv.domain.BiddingTime">
		insert into
		bidding_time(bidding_id,start_time,end_time,total_number)
		values(#{biddingId},#{startTime},#{endTime},#{totalNumber})
	</insert>

	<delete id="deleteByBiddingId" parameterType="String">
		delete from
		bidding_time where bidding_id=#{biddingId}
	</delete>

</mapper>